{
  "name": "lampix-app-starter",
  "version": "1.0.0",
  "description": "Boilerplate for starting Lampix apps",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config internals/webpack.prod.js --progress --colors",
    "start": "webpack-dev-server -d --inline --hot --config internals/webpack.dev.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-pattern internals/",
      "git add"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "@lampix/core": "^1.0.0-beta.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.4.0",
    "lint-staged": "^6.1.1",
    "node-sass": "^4.9.4",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3"
  }
}
